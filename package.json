{
  "name": "@s2g/ipfs-pinning-adapter",
  "version": "0.0.2",
  "description": "Framework for unifying different IPFS pinning services",
  "repository": "https://github.com/s2gprotocol/ipfs-service-adapter",
  "author": "s2g.network",
  "license": "Apache-2.0",
  "types": "index.d.ts",
  "module": "./index.js",
  "main": "./index.js",
  "type": "module",
  "scripts": {
    "build": "yarn build:dev",
    "build:dev": "tsc",
    "build:prod": "rimraf dist && tsc && cp package.json dist/ && cp LICENSE dist/ ",
    "test": "node ./node_modules/.bin/jest test  -c jest.config.unit.js --runInBand --forceExit",
    "test:integration": "node ./node_modules/.bin/jest test -c jest.config.integration.js --runInBand --forceExit",
    "pretest": "yarn build",
    "lint": "eslint src/**/*.ts",
    "pretty": "prettier --write src/**/*.ts package.json",
    "pretty:check": "yarn prettier --loglevel error --check  src/**/*.ts package.json",
    "fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {},
  "peerDependencies": {
    "@pinata/sdk": "^1.1.23",
    "ipfs-car": "^0.6.1",
    "nft.storage": "5.2.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0"
  },
  "devDependencies": {
    "@pinata/sdk": "^1.1.23",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.13",
    "ipfs-car": "^0.6.1",
    "jest": "^27.0.0",
    "nft.storage": "5.2.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.0.5",
    "dotenv": "^15.0.0"
  }
